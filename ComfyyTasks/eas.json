{
    "cli": {
        "version": ">= 12.5.0",
        "appVersionSource": "remote"
    },
    "build": {
        "development": {
            "developmentClient": true,
            "distribution": "internal"
        },
        "preview": {
            "distribution": "internal"
        },
        "production": {
            "autoIncrement": true,
            "env": {
                "EXPO_PUBLIC_API_URL": "https://api.production.com"
            }
        }
    },
    "submit": {
        "production": {}
    }
}
